<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!--提供方信息-->
    <dubbo:application name="dubbo-client" owner="yizhuo"/>

    <!--注册中心-->
    <dubbo:registry address="zookeeper://zookeeper.com:2181" file="d:/dubbo-service"/>

    <dubbo:reference id="helloService" interface="com.zhuyizhuo.java.IHelloService"
                     mock="com.zhuyizhuo.java.mock.MockHelloServiceImpl" timeout="10"/>

</beans>